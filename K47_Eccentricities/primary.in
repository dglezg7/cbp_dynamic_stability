# The host star: Kepler 47A
# Parameters from Leung & Lee (2013)
sName		primary		# Body's name
sOutFile	K47_e0pt100.primary.forward
saModules binary eqtide stellar
#Place "binary" before "eqtide" to turn on cbp

# Physical Parameters
iBodyType       1               # 1 -> Star
dMass		1.043                # Mass, negative -> Earth masses
dRotPeriod      -4.0            # Rotation period (-1 -> days)
bEvolveRG       1
dRadGyra        0.27 
dAge		1.0e6

# EQTIDE Parameters
dTidalTau		3.17e-09  # Tidal time lag
dK2		0.5		         # Love number of degree 2
dMaxLockDiff    0.01
sTideModel	t8		   # Tidal model, p2=CPL, t8=CTL
saTidePerts	secondary	# Body name(s) of tidal perturbers

# STELLAR Parameters
sStellarModel baraffe
sMagBrakingModel matt

saOutputOrder	Time -Radius -RotPer -RotRate -LostAngMom RadGyra -SurfEnFluxTotal -CriticalSemiMajorAxis -LockTime 
